#include "holberton.h"
/**
 * *_strspn - get the length of a prefix substring.
 * @s: sting.
 * @accept: string.
 * Return: dest.
 */
unsigned int _strspn(char *s, char *accept)
{
int i = 0, j = 0;
while (s++)
{
for (j = 0; accept[j] != '\0'; j++)
{
if (*s = accept[j])
  {
i++;
  }
}
}
return (i);
}
